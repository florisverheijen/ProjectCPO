   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "V = np.array([[3,5]])\n",
    "origin = [0], [0] # origin point\n",
    "\n",
    "plt.quiver(*origin, V[:,0], V[:,1], color=['b'] , scale=50)\n",
    "\n",
    "\n",
    "W = 3*V\n",
    "\n",
    "plt.quiver(*origin, W[:,0], W[:,1], color=['r'] , scale=50)\n",
    "\n",
    "\n",
    "Z= -1*V\n",
    "\n",
    "plt.quiver(*origin, Z[:,0], Z[:,1], color=['g'] , scale=50)\n",
    "plt.show()\n"
   ]
